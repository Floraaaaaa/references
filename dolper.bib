@Article{黄文鹂2018p22--2740,
  author        = {黄文鹂 and 陈建文 and 鲍拯},
  title         = {天波雷达弱目标检测的知识辅助DP-TBD方法},
  journal       = {华中科技大学学报（自然科学版）},
  year          = {2018},
  number        = {5},
  pages         = {22--27,40},
  __markedentry = {[junhu:]},
  abstract      = {提出一种知识辅助动态规划检测前跟踪(KA-DP-TBD)方法.在天波雷达弱目标的检测中,联合多帧回波数据积累能量,先单帧数据做常规信号处理,再用距离-多普勒平面上目标的运动特征及目标运动的统计特性为辅助知识,在下一帧能量积累时选择在合理的关联区域内进行动态规划积累,排除不符合目标运动特性的积累区域的强噪声对检测结果的影响,提高雷达在复杂多变的外界干扰环境及非平稳的电离层传播环境中的检测概率,在提高弱目标检测性能的同时减小了运算量,工程可实现性较强.仿真分析和实测数据处理结果验证了该方法的有效性.},
  doi           = {10.13245/j.hust.180505},
  keywords      = {天波超视距雷达; 弱目标; 知识辅助; 动态规划TBD方法; 检测前跟踪},
  url           = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=hzlgdxxb201805005},
}

@Article{邢强2017p48--52,
  author   = {邢强 and 贾鑫 and 薄远 and 周赤},
  title    = {LFM雷达全脉冲参差分段转发干扰方法},
  journal  = {电子信息对抗技术},
  year     = {2017},
  number   = {3},
  pages    = {48--52},
  abstract = {线性调频(LFM)脉冲压缩雷达输出响应产生与多普勒频移成正比的附加时延,鉴于此,提出了对接收脉冲信号参差分段的转发干扰方法,通过对雷达信号的参差分段并重新排序转发,经过匹配滤波后形成多相干假目标.揭示了全脉冲参差分段转发干扰的基本原理;推导了假目标峰值、主瓣宽度与分段子脉宽的关系;给出了产生有效假目标时的分段子脉宽取值范围;最后,对该方法产生的假目标进行了仿真实验,仿真结果表明该方法能产生有效的相干距离假目标.该方法产生的假目标会有延迟,在实际应用中需要干扰机前置、预测转发或其他方法进行补偿.},
  doi      = {10.3969/j.issn.1674-2230.2017.03.011},
  keywords = {LFM; 脉冲压缩; 转发干扰; 参差分段, rank3},
  url      = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=dzdkjs201703011},
