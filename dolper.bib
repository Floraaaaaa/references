@Article{丛潇雨2017p1637--1646,
  author        = {丛潇雨 and 韩玉兵 and 盛卫星 and 张仁李},
  title         = {CPI回波Stretch处理雷达抗欺骗式干扰算法},
  journal       = {信号处理},
  year          = {2017},
  number        = {12},
  pages         = {1637--1646},
  __markedentry = {[junhu:]},
  abstract      = {由于射频存储技术的飞速发展,转发式的欺骗干扰成为影响雷达精确跟踪的一大威胁.针对欺骗式干扰,本文提出了一种基于CPI回波的Stretch处理抗干扰算法,该算法利用前一个相干处理周期(CPI)跟踪得到的距离和速度值,估计当前CPI的时延和多普勒频率,构造基带回波并与当前接收到的CPI信号进行Stretch处理,将原本相干的目标信号与干扰信号从频域上分离,将处于高频处的干扰滤除,然后进行Stretch逆处理,恢复出只含有目标的信号,送入动目标检测(MTD)处理和恒虚警检测模块.仿真结果表明,采用本文所提出的抗干扰算法,在单个假目标干扰和密集欺骗式干扰的场景下,都可以有效抑制干扰,并且该算法在机动目标,多散射中心目标和多目标跟踪的情景下仍然适用,可以正确跟踪目标信号.},
  doi           = {10.16798/j.issn.1003-0530.2017.12.016},
  keywords      = {射频存储; 欺骗干扰; Stretch处理; 抗干扰},
  url           = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=xhcl201712016},
}

@Article{吴磊2017p25--30,
  author        = {吴磊 and 刘景萍 and 张聚栋 and 王倩},
  title         = {有源压制式干扰对MIMO引信测距测速的影响},
  journal       = {探测与控制学报},
  year          = {2017},
  number        = {6},
  pages         = {25--30},
  __markedentry = {[junhu:]},
  abstract      = {针对有源干扰对MIMO引信影响较大的现状,从干扰波形方面分析干扰机理,并从信噪比增益方面对比分析了三种常见有源干扰对基于相位编码MIMO引信测距测速的影响.理论分析表明当噪声干扰带宽与回波信号带宽重叠时,即当干扰频率与载频相差多普勒频率时,干扰信号类似于目标回波信号,对引信测距测速影响最大.噪声调幅、噪声调频、噪声调相这三种有源干扰中,噪声调幅对测距测速影响最大.利用Mat-lab构建基于相位编码体制MIMO引信信号处理平台,仿真结果验证了理论分析的正确性,为相关工作者以后提出抗干扰算法提供了理论依据.},
  keywords      = {MIMO引信; 有源干扰; 干扰波形; 相位编码},
  url           = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=tcykzxb201706006},
}

@Article{陈文东2018p107--113,
  author        = {陈文东 and 贺志毅 and 闫雅琼},
  title         = {采用正交分集的灵巧噪声干扰对抗方法},
