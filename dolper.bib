  journal       = {现代电子技术},
  year          = {2017},
  number        = {16},
  pages         = {132--134,139},
  __markedentry = {[junhu:]},
  abstract      = {传统的设备信道选取模型分析方法复杂、模型模拟器电路过于简化,应加以改进.提出并设计一种移动通信设备多信道选取模型并改进.介绍模型模拟器的组成以及仿真原理,通过改变移动通信距离偏角并添加白噪声,排除模型中的多普勒效应和通信噪声.改进模型模拟器A/D电路中的可控增益放大器,提高多信道中信号的干扰排除操作强度.用分布式计算系统替换原模型中的密集型计算系统,提高模型计算精度.实验结果表明,改进后的模型取得了更高的电平过载率和更短的衰减时间.},
  doi           = {10.16652/j.issn.1004-373x.2017.16.039},
  keywords      = {移动通信设备; 多信道选取模型; 多普勒效应排除; 模型设计},
  url           = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=xddzjs201716039},
}

@Article{魏世乐2017p123--128,
