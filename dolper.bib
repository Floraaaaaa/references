  keywords      = {多普勒天气雷达; 径向干扰回波; 去除; 对比},
  url           = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=ghqx201703021},
}

@Article{仇洪冰2018p123--131,
