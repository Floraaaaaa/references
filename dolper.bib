@Article{周义明2017p168--171,
  author        = {周义明 and 田小平 and 李英顺 and 李洋},
  title         = {感知空闲频谱的多普勒效应估计算法},
  journal       = {信息技术},
  year          = {2017},
  number        = {12},
  pages         = {168--171},
  __markedentry = {[junhu:]},
  abstract      = {在认知无线电的空闲频段二次利用中,针对感知用户通信可能引起的多普勒效应对临近信道带来的干扰的问题,提出了一种基于多普勒频谱扩展程度量化的实时计算方法.首先利用平方根升余弦函数使训练码频谱限定为特定带宽,并将感知空闲频谱划分为基带频段和保护频段,其次将保护频段划分为多个保护频段子区间,分别计算各个保护频段子区间的信号功率,最后累加信号功率不为零的保护频段子区间,即为整个信号的频谱扩展程度.文中方法同时也满足了对空闲频段实时检测的要求.仿真验证了在不同移动速度条件下,基于保护频段子区间的信号谱估计来计算多普勒频谱扩展的程度.},
  doi           = {10.13274/j.cnki.hdzj.2017.12.037},
  keywords      = {认知无线电; 空闲频谱; 频段子区间划分; 多普勒功率谱},
  url           = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=xxjs201712037},
}

@Article{张云鹏2017p84--90,
  author        = {张云鹏 and 毕大平 and 周阳 and 韩佳辉},
  title         = {对抗SAR-GMTI的方位向间歇采样延时转发干扰方法},
  journal       = {探测与控制学报},
  year          = {2017},
  number        = {5},
