  pages         = {401--409},
  __markedentry = {[junhu:]},
  abstract      = {针对宽带噪声干扰在空时自适应处理(Space-Time Adaptive Processing,STAP)雷达功率凝聚工作模式下无法有效掩护被保护目标方位信息的问题,提出了一种投散射式伪杂波干扰方法,分析了干扰信号在雷达接收端的空时特性.该方法通过向地面特定区域投射经过调制转发的雷达信号产生类杂波干扰信号.该信号多普勒频率与方位耦合、具有空间连续性与时间连续性,空时功率频谱轨迹可控.推导了干扰信号空时矢量模型,从雷达接收机的角度分析了干扰给目标检测带来的影响;在研究干扰信号多普勒轨迹分布特性与等值多普勒曲线分布特性的基础上,提出了干扰空时协方差矩阵特征值数目估计方法.仿真结果表明,该方法产生的干扰空时功率谱呈背脊线分布,使STAP处理器出现多个凹口,有效保护了目标信息.},
  doi           = {10.3969/j.issn.0372-2112.2018.02.020},
  keywords      = {空时自适应处理; 杂波; 投散射; 空间连续性; 多普勒轨迹; 特征谱, rank3},
  url           = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=dianzixb201802020},
}

@Article{李征2017p46--50,
